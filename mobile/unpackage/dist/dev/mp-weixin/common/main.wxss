/*每个页面公共css */
.dir-left-nowrap {
  /* 主轴 排列方式从左侧开始 不换行*/
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.dir-left-wrap {
  /* 主轴 排列方式从左侧开始 换行*/
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.dir-left-wrap-reverse {
  /* 主轴 排列方式从左侧开始 换行 第一行在下方*/
  display: flex;
  flex-direction: row;
  flex-wrap: wrap-reverse;
}
.dir-right-nowrap {
  /* 主轴 排列方式从 右侧开始 */
  display: flex;
  flex-direction: row-reverse;
  flex-wrap: nowrap;
}
.dir-right-wrap {
  /* 主轴 排列方式从 右侧开始 换行*/
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.dir-right-wrap-reverse {
  /* 主轴 排列方式从 右侧开始 换行 第一行在下方*/
  display: flex;
  flex-direction: row;
  flex-wrap: wrap-reverse;
}
.dir-top-nowrap {
  /* 主轴 排列方式从顶部开始 不换行 */
  display: flex;
  flex-direction: column;
  flex-wrap: nowrap;
}
.dir-top-wrap {
  /* 主轴 排列方式从顶部开始  换行*/
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}
.dir-top-wrap-reverse {
  /* 主轴 排列方式从顶部开始 不换行换行 第一行在下方*/
  display: flex;
  flex-direction: column;
  flex-wrap: wrap-reverse;
}
.dir-bottom-nowrap {
  /* 主轴 排列方式从底部开始 */
  display: flex;
  flex-direction: column-reverse;
  flex-wrap: nowrap;
}
.dir-bottom-wrap {
  /* 主轴 排列方式从底部开始 不换行 换行*/
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}
.dir-bottom-wrap-reverse {
  /* 主轴 排列方式从底部开始 不换行换行 第一行在下方*/
  display: flex;
  flex-direction: column;
  flex-wrap: wrap-reverse;
}
.main-left {
  /* 主轴 左对齐 */
  display: flex;
  justify-content: flex-start;
}
.main-right {
  /* 主轴 右对齐 */
  display: flex;
  justify-content: flex-end;
}
.main-between {
  /* 主轴 两端对齐 */
  display: flex;
  justify-content: space-between;
}
.main-center {
  /* 主轴 居中对齐 */
  display: flex;
  justify-content: center;
}
.main-around {
  /* 主轴 项目位于各行之前、之间、之后都留有空白的容器内*/
  display: flex;
  justify-content: space-around;
}
.cross-top {
  /* 交叉轴 起点对齐 */
  display: flex;
  align-items: flex-start;
}
.cross-bottom {
  /* 交叉轴 终点对齐 */
  display: flex;
  -webkit-box-align: end;
  -webkit-align-items: flex-end;
  -ms-flex-align: end;
  -ms-grid-row-align: flex-end;
  align-items: flex-end;
}
.cross-baseline {
  /* 交叉轴 第一行文字基线对齐 */
  display: flex;
  align-items: baseline;
}
.cross-center {
  /* 交叉轴 居中对齐 */
  display: flex;
  align-items: center;
}
.cross-stretch {
  /* 交叉轴 高度并排铺满 高度不固定*/
  display: flex;
  align-items: stretch;
}
.flex-wrap {
  /* 流模式 第一行在上方 */
  display: flex;
  flex-wrap: wrap;
}
.flex-wrap-reverse {
  /* 流模式 第一行在下方 */
  display: flex;
  flex-wrap: wrap-reverse;
}
.box-grow-0 {
  /* flex 子元素固定宽度*/
  min-width: 0;
  flex-grow: 0;
  flex-shrink: 0;
}
.box-grow-1 {
  /* flex 子元素等分 */
  min-width: 0;
  flex-grow: 1;
  flex-shrink: 1;
}
.inline-flex {
  display: inline-flex;
}
